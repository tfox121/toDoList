{% extends "index.html" %}

{% block list %}
<div class="columwrapper">
  <form action="/" method="post">
    {%- for item in data %}
      <li class="{{ item._id }}">{{ item.task }}
        <input type="submit" name="{{ item._id }}" value="X" />
      </li>
    {%- endfor %}
  </form
</div>
{% endblock %}